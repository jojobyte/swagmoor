    "major": "cargo --manifest-path src-tauri/Cargo.toml release major --execute"
  },
  "dependencies": {
    "@tauri-apps/api": "^1.0.2"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^1.0.5",
